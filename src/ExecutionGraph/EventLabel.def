HANDLE_CAS_READ_LABEL(24, TrylockCasRead)
HANDLE_CAS_READ_LABEL(25, HelpedCasRead)
HANDLE_CAS_READ_LABEL(26, ConfirmingCasRead)
LAST_CAS_READ_LABEL(26)

